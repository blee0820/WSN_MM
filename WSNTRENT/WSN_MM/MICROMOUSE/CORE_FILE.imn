node n4 {
    type router
    model router
    network-config {
	hostname n4
	!
	interface eth0
	 ip address 10.0.0.4/32
	 ipv6 address 2001:0::4/128
	!
    }
    canvas c2
    iconcoords {410.0 345.0}
    labelcoords {410.0 377.0}
    interface-peer {eth0 n5}
